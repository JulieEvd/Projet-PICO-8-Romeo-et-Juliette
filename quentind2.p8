-- support functions
function switch_value(boolval)
	if boolval == true then
		boolval = false
	elseif boolval ==false then
		boolval = true
	end
end

function reset_combat_val(player)
	player.can_atk=true
	player.can_def=true
	player.attacking=false
	player.defending=false
	player.is_blocked=false
end

