--player animate
function player_animate(player)
	
	if player.landed then
		player.sp = player.sp_landed
		if player.attacking then
			player.sp = player.sp_atk
		elseif player.defending then
			player.sp = player.sp_def
		end
		if player.ko then
			player.sp = player.sp_ko
		end
	elseif player.jumping or player.falling then
		player.sp = player.sp_jump
		if player.attacking then
			player.sp = player.sp_atk
		elseif player.defending then
			player.sp = player.sp_def
		end
		if player.ko then
			player.sp = player.sp_ko
		end		
	end
	
	
	spr(player.sp,player.x,player.y,player.w/8,2,player.facing=='left')
end
		 
