--update and draw
function _update60()
	player_update(p1)
	player_update(p2)
	next_room()
end

function _draw()
	cls()
	map(0,0)
	camera(mapx,mapy)
	spr(p1.sp,p1.x,p1.y,p1.w/8,2)
	spr(p2.sp,p2.x,p2.y,p2.w/8,2)
end
