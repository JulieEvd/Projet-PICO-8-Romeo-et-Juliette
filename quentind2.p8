-- attack logic
function players_alignement()
		p1.facing, p2.facing = 'right', 'right'
		if p1.x > p2.x then
		p1.facing='left' else p2.facing='left'
	end
end

function attack(player)
	player.attacking=true
	player.can_atk=false
	player.can_def=false
	player.reset_timer=time()+atk_timeout
end

function defend(player)
	player.defending=true
	player.can_def=false
	player.can_atk=false
	player.reset_timer=time()+def_timeout
end

function check_attack(player)
	-- check attack
	-- end of flower must touch body part of other player
	for p in all(players) do
		if p != player then
			if player.facing == 'right' then
				if (((player.x + player.w >= p.x + p.w/2) and (player.x + player.w/2 <= p.x + p.w)) and (player.y+player.h/2 >= p.y and player.y+player.h/2+2 <= p.y+p.h)) then
					return true
				end
			elseif player.facing =='left' then
				if ((player.x <= p.x+p.w/2) and (player.x + player.w/2 >= p.x) and  (player.y+player.h/2 >= p.y and player.y+player.h/2+2 <= p.y+p.h)) then
					return true
				end
			end

		end
	end
	return false
end

function resolve_double_attack()
	--triggered when both players attack and hit
	p1.attacking, p2.attacking = false, false
	reset_combat_val(p1)
	reset_combat_val(p2)
	if p1.facing == 'right' then
		p1.dx -= 2
		p2.dx += 2
	else 
		p1.dx += 2
		p2.dx -= 2		
	end
end

function got_blocked(player)
	player.attacking=false
	player.defending=false
	player.can_atk=false
	player.can_def=false
	player.is_blocked=true
	player.reset_timer=time()+1.5
end

function got_ko(player)
	player.attacking=false
	player.defending=false
	player.can_atk=false
	player.can_def=false
	player.ko=true
	player.y-=8
	--player.h=8
	player.respawn_timer=time()+3
end

