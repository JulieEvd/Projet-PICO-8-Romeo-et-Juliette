--player
function player_update(player)
	--physics
	player.dy+=gravity
	player.dx*=friction

	--attack
	if player.attacking then
		player.sp=35
		player.w=16
		if time()>player.attack_timer then
			player.attacking=false
			player.sp=1
			player.w=8
		end
	end

	--controls
	
	if btn(⬅️,player.player_key) then
		player.dx-=player.acc
		player.running=true		
	end
		
	if btn(➡️,player.player_key) then
		player.dx+=player.acc
		player.running=true		
	end
		
	--jump
	if btnp(⬆️,player.player_key)
		and player.landed then
			player.sp=player.sp_jump
			player.dy-=player.boost
			player.landed=false
	end
	
	--attack
	if btnp(❎,player.player_key)
	 and player.attacking==false then
		attack(player)
	end	 

	--check collision up and down
	if player.dy>0 then
			player.falling=true
			player.landed=false
			player.jumping=false
			
			if collide_map(player,"down",0) then
				player.landed=true
				player.sp=player.sp_landed
				player.falling=false
				player.dy=0
				player.y-=(player.y+player.h)%8
			end
				
			elseif player.dy<0 then
				player.jumping=true
				if collide_map(player,"up",0) then
					player.dy=0
			end

	end
	--check collision left and right
	
	if player.dx<0 then
		if collide_map(player,"left",1) then
			player.dx=0
		end
	
	elseif player.dx>0 then
		if collide_map(player,"right",1) then
			player.dx=0
		end
	end

		player.x+=player.dx
		player.y+=player.dy
end	

		
		 
