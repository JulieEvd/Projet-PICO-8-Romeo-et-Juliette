--collisions

function collide_map(obj,aim,flag)
	--obj = table needs x,y,w,h
	
	local x=obj.x		local y=obj.y
	local w=obj.w		local h=obj.h
	
	local x1=0		local y1=0
	local x2=0		local y2=0
	
	if aim=="left" then
		if obj.facing==aim then
			x1=x+w/2-2		y1=y
			x2=x+w/2		y2=y+h-1
		else
			x1=x-1		y1=y
			x2=x	y2=y+h-1
		end
	
	elseif aim=="right" then
		if obj.facing==aim then
			x1=x+w/2	y1=y
			x2=x+w/2+1	y2=y+h-1
		else
			x1=x+w 		y1=y
			x2=x+w+1	y2=y+h-1
		end
	
	elseif aim=="up" then
		if obj.facing=='left' then
			x1=x+w/2-1		y1=y-1
			x2=x+w			y2=y
		else
			x1=x+w/2-1		y1=y-1
			x2=x			y2=y
		end
	
	elseif aim=="down" then
		if obj.facing=='left' then
			x1=x+w/2-1		y1=y+h
			x2=x+w			y2=y+h
		else
			x1=x+w/2-1		y1=y+h
			x2=x			y2=y+h
		end
	end

	--pixels to tiles
	x1/=8		y1/=8
	x2/=8		y2/=8

	if fget(mget(x1,y1), flag)
	or fget(mget(x1,y2), flag)
	or fget(mget(x2,y1), flag)
	or fget(mget(x2,y2), flag) then
		return true
	else
		return false
	end
	
end

