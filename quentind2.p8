--player update
function player_update(player)

	--physics
	player.dy+=gravity
	player.dx*=friction
	player.acc=base_acc
	--the player that was parried is slew down
	if player.is_blocked then
		player.acc*=0.75
	end

	--respawn
	if player.ko and time()>player.respawn_timer then
		player.ko=false
		player.h=16
		if player==p1 then
			player.x=camx+5
		elseif player==p2 then
			player.x=camx+104
		end
		reset_combat_val(player)
	end

	--controls
	if player.ko==false	then	
		if btn(⬅️,player.player_key) then
			player.dx-=player.acc
			player.running=true		
		end
			
		if btn(➡️,player.player_key) then
			player.dx+=player.acc
			player.running=true		
		end
			
		if btnp(⬆️,player.player_key)
			and player.landed then
				player.sp=player.sp_jump
				player.dy-=player.boost
				player.landed=false
				sfx(0)
		end

		if btnp(🅾️, player.player_key)
			and player.can_def==true then
				defend(player)
		end
		
		if btnp(❎,player.player_key)
			and player.can_atk==true then
				attack(player)
				sfx(1)
		end	
	end

	--alignement
	players_alignement()

	--attack
		--end of attack /defense/ blocked ; reset combat values	
	if time()>player.reset_timer and (player.attacking or player.defending or player.is_blocked) then
		reset_combat_val(player)
	end
		-- attack collision cases
	if player.attacking and check_attack(player) then
		if player.other_player.ko==false then	
			if player.other_player.attacking then
				resolve_double_attack()
			elseif player.other_player.defending then
				got_blocked(player)
			else
				got_ko(player.other_player)
			end
		end
	end

	--players collision
	

	--check collision up and down
	if player.dy>0 then
			player.falling=true
			player.landed=false
			player.jumping=false
			
			if collide_map(player,"down",0) then
				player.landed=true
				player.sp=player.sp_landed
				player.falling=false
				player.dy=0
				player.y-=(player.y+player.h)%8
			end
				
			elseif player.dy<0 then
				player.jumping=true
				if collide_map(player,"up",0) then
					player.dy=0
			end

	end
	--check collision left and right
	
	if player.dx<0 then
		if collide_map(player,"left",1) then
			player.dx=0
		end
	
	elseif player.dx>0 then
		if collide_map(player,"right",1) then
			player.dx=0
		end
	end

		player.x+=player.dx
		player.y+=player.dy
end

